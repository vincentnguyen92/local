#!/bin/bash

source bin/.script/init.sh;

greeting;

read YOUR_CHOICE;

case $YOUR_CHOICE in
	1)
		create_new_site
		;;
	2)
		echo -en "\n";
		echo "<--- Being under construction..^^ --->";
		read;
		;;
	3)
		echo -en "\n";
		echo "<--- Cleaning all project --->";
		read;
		;;
	*)
		echo "Hmm, wrong number....."
		;;
esac
exit;